<!--xndir1 palindrom>
let arr="qaxaq";
let scope1=true;
let scope=true;
let num=Math.trunc(arr.length/2);//2
let num1=arr.length-1;//4
let i,j;
for(i=0,j=num1;i<num,j>num;i++,j--){
	if(arr[i]==arr[j]){scope=true;}
	else{scope=false;}
	scope1=scope1&&scope;
}	 
if(scope1){console.log("palindrom e");}
else{console.log("palindrom che");}
