<!--xndir8>
let arr=[ 4, 7, 10, 11, 13 ];
let newarr = [];
for(let i=0;i<arr.length;i++){
   if(arr[i]%2!=0){
   newarr.push(arr.indexOf(arr[i]));
   }
}console.log(newarr);
