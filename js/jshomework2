<!--xndir9>
let arr = [1,4,6,3,2,7];
let num=arr.length;
let cast=0;
for(let  i=0;i<num;i++){
   for(let j=0;j<num;j++){
     if(arr[j]>arr[j+1]){
     cast=arr[j+1];
     arr[j+1]=arr[j];
     arr[j]=cast;
     }
   }num--;
}console.log(arr);
